    "private": true,
    "dependencies": {
        "@sap/cds": "^7.9.0",
        "@sap/cds-hana": "^2",
        "@sap/xssec": "^3",
        "express": "^4",
        "passport": "^0"
    },
    "devDependencies": {
        "@cap-js/sqlite": "^1",
        "@sap/cds-dk": "^7",
        "@sap/ux-specification": "^1.120.10",
        "rimraf": "^3.0.2"
    },
    "scripts": {
        "start": "cds-serve",
        "watch-vendor_request_cap_application": "cds watch --open vendor_request_cap_application/webapp/index.html?sap-ui-xx-viewCache=false",
        "undeploy": "cf undeploy Vendor_request_annoel --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
    },
    "cds": {
        "requires": {
            "[production]": {
                "db": "hana",
                "auth": "xsuaa"
            }
        }
    },
    "sapux": [
        "app/vendor_request_cap_application"
    ]
